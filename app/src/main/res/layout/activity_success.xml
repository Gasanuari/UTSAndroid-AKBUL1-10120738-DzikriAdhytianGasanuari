    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Berhasil Disimpan!"
        android:textSize="18sp" />

    <Button
        android:id="@+id/btnOk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OK" />
</LinearLayout>

